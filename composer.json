{
    "name": "rawilk/laravel-ups",
    "description": "UPS api wrapper for Laravel.",
    "keywords": [
        "rawilk",
        "laravel-ups"
    ],
    "homepage": "https://github.com/rawilk/laravel-ups",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Wilk",
            "email": "randall@randallwilk.dev",
            "homepage": "https://randallwilk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Rawilk\\LaravelUps\\": "src",
            "Rawilk\\LaravelUps\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawilk\\LaravelUps\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawilk\\LaravelUps\\LaravelUpsServiceProvider"
            ],
            "aliases": {
                "LaravelUps": "Rawilk\\LaravelUps\\Facades\\LaravelUpsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
